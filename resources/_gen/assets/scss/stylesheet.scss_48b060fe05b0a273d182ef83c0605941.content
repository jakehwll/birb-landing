@import "https://fonts.googleapis.com/css?family=Open+Sans&display=swap";*{box-sizing:border-box}body,html{background:#37393f;color:#dcddde;font-size:16px;font-family:open sans,sans-serif;margin:0;padding:0}.container{max-width:800px;margin:0 auto;padding:0 1rem;text-align:center}img{background:#2f3136;border:1.5px solid #202225;border-radius:100%;margin-top:3rem}h1,h2,h3,h4,h5,h6{color:#fff}pre{font-size:.875rem;line-height:1.125rem;text-indent:0;white-space:pre-wrap;background:#2f3136;border:1px solid #202225;padding:.5em;border-radius:4px;color:var(--header-secondary);text-align:left}.btn{padding:1rem;background:#c87b3e;border:1px solid #905729;color:#fff;margin:.5rem 0;display:inline-block;text-decoration:none;border-radius:4px;transition:1s ease;box-shadow:0 4px 25px rgba(0,0,0,.1);overflow:auto;width:100%}.btn.alt{background:#407aa3;border:1px solid #2a516c}.btn:hover{transition:1s ease;transform:translateY(-0.25rem);box-shadow:0 4px 25px rgba(0,0,0,.25)}footer{padding:2rem 0;background:#2f3136;border-top:1px solid #202225}footer a{color:#fff;text-decoration:none}